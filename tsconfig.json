{
  "extends": "@vue/tsconfig/tsconfig.json",
  "include": [ 
    "resources/**/*.ts",
    "resources/**/*.d.ts",
    "resources/**/*.vue",
    "auto-imports.d.ts",
    "components.d.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["resources/js/*"],
      "#build/ui": [
        "./node_modules/@nuxt/ui/.nuxt/ui"
      ]
    },
    "types": ["vite/client"]
  }
}